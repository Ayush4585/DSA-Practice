class Solution {
public:
    int maximizeSquareHoleArea(int nn, int mm, vector<int>& hBars, vector<int>& vBars) {
        int count1=1,count2=1,n=hBars.size(),m=vBars.size(),currCount1=1,currCount2=1;
        sort(hBars.begin(),hBars.end());
        sort(vBars.begin(),vBars.end());
        for(int i=0;i<n-1;i++){
            if(hBars[i]==hBars[i+1]-1){
                currCount1++;
                count1=max(count1,currCount1);
            }
            else {
                currCount1=1;
            }
        }
        for(int i=0;i<m-1;i++){
            if(vBars[i]==vBars[i+1]-1){
                currCount2++;
                count2=max(count2,currCount2);
            }
            else {
                currCount2=1;
            }
        }
        int ans=min(count1,count2)+1;
        return ans*ans;
    }
};
