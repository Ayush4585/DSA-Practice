//O(N-n),  N = 1224444
class Solution {
public:
    bool check(int num) {
        vector<int> freq(10);
        while(num > 0) {
            int digit = num%10;
            freq[digit]++;
            num /= 10;
        }
        for(int d = 0; d < 10; d++) {
            if(freq[d] != 0 && freq[d] != d)
                return false;
        }  
        return true;
    }
    int nextBeautifulNumber(int n) {
        for(int num = n+1; num <= 1224444; num++) {
            if(check(num)) {
                return num;
            }
        }
        return -1;
    }
};
